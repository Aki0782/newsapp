{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "skipLibCheck": true,
    "noUnusedLocals": true,
    "isolatedModules": false,
    "baseUrl": ".",
    "paths": {
      "@Api": [
        "src/Api"
      ],
      "@Api/*": [
        "src/Api/*"
      ],
      "@Callbacks": [
        "src/CallBacks"
      ],
      "@Components/*": [
        "src/Components/*"
      ],
      "@Assets/*": [
        "src/Assets/*"
      ],
      "@Constants/*": [
        "src/Constants/*"
      ],
      "@Utils": [
        "src/Utils"
      ],
      "@Pages": [
        "src/Pages"
      ],
      "react": [
        "node_modules/@types/react"
      ]
    }
  }
}